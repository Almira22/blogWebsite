
<%- include("partials/header"); -%>
    <h1>Home</h1>
    <h3> <%= startingContent %> </h3>


  <%  posts.forEach(function(post){ %>

    <h1><%=post.title%></h1>
    <p>
   
      <%= (post.content)? post.content.substring(0, 100) + '...' : 'No content available' %>
    <a href="/posts/<%=post.id%>">Read More</a>
    </p>


    <% }) %>


<%- include("partials/footer"); -%>
