
<%- include("partials/header"); -%>
    <h1 style="color:rgb(35, 78, 35)">Home</h1>
    <h3 style="color:rgb(48, 160, 48)"> &#129312 <%= startingContent %> </h3>


  <%  posts.forEach(function(post){ %>

    <h1><%=post.title%></h1>
    <p>
   
      &#128154 <%= (post.content)? post.content.substring(0, 100) + '...' : 'No content available' %>
    <a href="/posts/<%=post.id%>">Read More</a>
    </p>


    <% }) %>


<%- include("partials/footer"); -%>
